
@{
    ViewBag.Title = "WorkAdd";
    Layout = "~/Areas/AdminPanel/Views/Shared/_Layout.cshtml";
}

<form action="@Url.Action("WorkAdd","Work")" method="post" enctype="multipart/form-data" ">
    <span>İş Ekle</span>

    <div class="x_panel" style="margin-top:15px;">
        <input type="text" id="name" class="form-control" name="Name" placeholder="İş Adı Giriniz..">
        <input type="text" id="branch" class="form-control" name="Branch" placeholder="Görev Giriniz..">
        <input type="text" id="dateofstart" class="form-control" name="DateOfStart" placeholder="Başlangıç Tarihi..">
        <input type="text" id="dateofend" class="form-control" name="DateOfEnd" placeholder="Bitiş Tarihi..">
    </div>

    <div class="form-group">
        <a href="@Url.Action("Index","Home")" class="btn btn-default pull-right">IPTAL</a>
        <input type="submit" value="Ekle" class="btn btn-primary pull-right" />
    </div>
</form>
